{
  "name": "use-cap",
  "version": "1.0.0",
  "keywords": [
    "react",
    "nodejs",
    "captcha",
    "pow",
    "proof-of-work",
    "spam",
    "bot",
    "protection",
    "token"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takeshape/use-cap.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean",
    "ci": "vitest run",
    "clean": "del-cli dist *.tsbuildinfo",
    "lint": "biome check --error-on-warnings",
    "prepare": "npm run build",
    "release": "np --no-publish",
    "test": "npm run format && npm run typecheck && vitest run",
    "typecheck": "tsc --noEmit",
    "watch": "vitest"
  },
  "devDependencies": {
    "@cap.js/wasm": "0.0.6",
    "@types/node": "^22.13.13",
    "@tsconfig/recommended": "^1.0.10",
    "@types/react": "^19.1.10",
    "@vitejs/plugin-react": "^5.0.1",
    "del-cli": "^5.1.0",
    "np": "10.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@cap.js/wasm": "0.0.6",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
